{
    "extends": "airbnb-base/legacy",
    "rules": {
        "indent": [
            "error",
            4
        ],
        "func-names": [
            "error",
            "never"
        ],
        "no-param-reassign": [
            "error",
            {
                "props": false
            }
        ]
    },
    "env": {
        "es6": true
    },
    "parserOptions": {
        "ecmaVersion": 6
    }
}